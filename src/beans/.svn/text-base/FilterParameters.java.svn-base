package beans;

public class FilterParameters {
	
	public FilterParameters()
	{
		
	}
	
	public FilterParameters(String degreeProgram,String gradYear,String gpa,String [] entCourses)
	{
		this.degreeProgram = degreeProgram;
		this.gradYear = gradYear;
		this.gpa = gpa;
		this.entCourses = entCourses;
	}
	
	private String degreeProgram;
	private String gradYear;
	private String gpa;
	private String [] entCourses;
	
	public String getDegreeProgram() {
		return degreeProgram;
	}
	public void setDegreeProgram(String degreeProgram) {
		if(degreeProgram == null)
			this.degreeProgram = "";
		this.degreeProgram = degreeProgram;
	}
	public String getGradYear() {
		return gradYear;
	}
	public void setGradYear(String gradYear) {
		if(gradYear == null)
			this.gradYear = "";
		this.gradYear = gradYear;
	}
	public String getGpa() {
		return gpa;
	}
	public void setGpa(String gpa) {
		if(gpa == null)
			this.gpa = "";
		this.gpa = gpa;
	}
	public String[] getEntCourses() {
		return entCourses;
	}
	public void setEntCourses(String[] entCourses) {
		this.entCourses = entCourses;
	}

}
